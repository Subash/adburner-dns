{
	"name": "adburner-dns",
	"version": "1.0.0",
	"private": true,
	"dependencies": {
		"dns-packet": "^5.2.1",
		"forever": "^1.0.0",
		"fs-extra": "^8.0.1",
		"is-ip": "^3.0.0",
		"is-url": "^1.2.4",
		"minimatch": "^3.0.4",
		"node-fetch": "^2.6.0"
	},
	"scripts": {
		"test": "jest",
		"build": "tsc -p .",
		"start": "forever dist/server.js",
		"start-dev": "sudo node dist/server.js",
		"docker-build": "docker build -t subash/adburner-dns .",
		"docker-build-arm": "docker build -t subash/adburner-dns:arm .",
		"docker-run": "docker run -p 53:53/udp --env-file .env.example subash/adburner-dns",
		"docker-publish": "docker push subash/adburner-dns:latest",
		"docker-publish-arm": "docker push subash/adburner-dns:arm"
	},
	"devDependencies": {
		"@types/node": "^12.7.5",
		"@types/node-fetch": "^2.5.0",
		"jest": "^24.8.0",
		"typescript": "^3.6.3"
	}
}